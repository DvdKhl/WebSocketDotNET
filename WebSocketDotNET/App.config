<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.diagnostics>
    <sharedListeners>
      <add name="xml" type="System.Diagnostics.XmlWriterTraceListener" initializeData="trace.xml" traceOutputOptions="DateTime,LogicalOperationStack,ThreadId"/>
    </sharedListeners>
    <sources>
      <source name="DvdKhl.WebSocket.WebSocketServer">
        <listeners>
          <add name="xml"/>
        </listeners>
      </source>
    </sources>
    <trace autoflush="true"/>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup></configuration>
